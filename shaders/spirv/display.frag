//#version 450#extension GL_ARB_explicit_attrib_location : enable#extension GL_ARB_separate_shader_objects : enable#extension GL_ARB_conservative_depth : enable#if __VERSION__ > 130layout(location = 0) out vec4 fragColor;#elif __VERSION__ == 130out vec4 fragColor;#endiflayout(binding = 0)  uniform sampler2D tex0;#if __VERSION__ > 120layout(location = 0) in vec2 uv;#elsevarying vec2 uv;#endifvoid main() {	#if defined(GL_ARB_explicit_attrib_location)	fragColor = texture(tex0, uv);	#else	gl_FragColor = texture2D(tex0, uv);	#endif}